def factorial(n):
    if n == 0:
        return 1
    else:
        return n * factorial(n-1)

factorial(100)





def gcd(m, n):
    if n == 0:
        return m
    return gcd(n, m%n)

gcd(91, 28)





def extended_euclid(m, n):
    """
    Liefert den ggT von m und n, sowie zwei ganze Zahlen x und y, so dass ggT = x*m + y*n ist.
    Input: m, n
    Output ggT (gcd), x, y
    """
    if n == 0:
        return (m, 1, 0)
    gcd, x1, y1 = extended_euclid(n, m%n)
    x = y1
    y = x1 - (m//n)*y1
    return (gcd, x, y)


